{
  "todohighlight.isEnable": true,
  "todohighlight.isCaseSensitive": true,
  "todohighlight.keywords": [
    { "text": "BUG:", "backgroundColor": "#FF0000", "color": "#FFF" },
    { "text": "REVIEW:", "backgroundColor": "#228BE6", "color": "#FFF" },
    { "text": "REMOVE:", "backgroundColor": "#E1306C", "color": "#FFF" },
    { "text": "CHECK:", "backgroundColor": "#FFA500", "color": "#FFF" },
    { "text": "HACK:", "backgroundColor": "#5F3DC4", "color": "#FFF" },
    { "text": "NOTE:", "backgroundColor": "#008000", "color": "#FFF" }
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.requireConfig": true,
  "prettier.configPath": ".prettierrc",
  "appService.zipIgnorePattern": ["node_modules{,/**}", ".vscode{,/**}"],
  "appService.defaultWebAppToDeploy": "/subscriptions/5f49b8db-29eb-46da-9fcc-b0c0ca7d7aa9/resourceGroups/Wissen_Engage/providers/Microsoft.Web/sites/mqg-style-counsel",
  "appService.deploySubpath": ".",
  "cSpell.words": ["formatjs", "sedol", "tabler", "tseslint"],
  "typescript.tsdk": "node_modules/typescript/lib",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
